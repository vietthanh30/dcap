    <id name="MemberID" column="MEMBER_ID" unsaved-value="-1" />

    <property name="MemberIndex">
      <column name="MEMBER_INDEX" not-null="true" />
    </property>

    <property name="ParentID">
      <column name="PARENT_ID" not-null="false" />
    </property>

    <property name="ChildrenID">
      <column name="CHILDREN_ID" not-null="false" />
    </property>

    <property name="Status">
      <column name="STATUS" not-null="true" />
    </property>

  </class>
</hibernate-mapping>