<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
xmlns="urn:nhibernate-mapping-2.2"
namespace="domain_lib.model"
assembly="domain_lib">
  <class name="MemberInfo" table="MEMBER_INFO" lazy="false">
    <id name="MemberID" column="MEMBER_ID" unsaved-value="-1">
      <generator class="identity" />
    </id>

    <property name="HoTen">
      <column name="HO_TEN" not-null="true" />
    </property>

    <property name="HoTenKd">
      <column name="HO_TEN_KD" not-null="false" />
    </property>

    <property name="NgaySinh">
      <column name="NGAY_SINH" not-null="false" />
    </property>

    <property name="SoCmnd">
      <column name="SO_CMND" not-null="true" />
    </property>

    <property name="NgayCap">
      <column name="NGAY_CAP" not-null="false" />
    </property>

    <property name="SoDienThoai">
      <column name="SO_DIEN_THOAI" not-null="false" />
    </property>

    <property name="DiaChi">
      <column name="DIA_CHI" not-null="false" />
    </property>

    <property name="GioiTinh">
      <column name="GIOI_TINH" not-null="false" />
    </property>

    <property name="SoTaiKhoan">
      <column name="SO_TAI_KHOAN" not-null="false" />
    </property>

    <property name="ChiNhanhNH">
      <column name="CHI_NHANH_NH" not-null="false" />
    </property>

    <property name="ImageUrl">
      <column name="IMAGE_URL" not-null="false" />
    </property>

    <property name="CreatedDate">
      <column name="CREATED_DATE" not-null="false" />
    </property>

    <property name="CreatedBy">
      <column name="CREATED_BY" not-null="false" />
    </property>

  </class>
</hibernate-mapping>